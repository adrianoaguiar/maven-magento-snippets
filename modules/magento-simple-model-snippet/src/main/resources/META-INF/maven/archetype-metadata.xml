<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="mymodule"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" partial="true">
  <requiredProperties>
    <requiredProperty key="magentoModuleName"/>
    <requiredProperty key="magentoModuleNameLowerCase"/>
    <requiredProperty key="magentoNameSpace"/>
    <requiredProperty key="magentoModuleType"/>
    <requiredProperty key="modelName"/>
    <requiredProperty key="modelNameLowerCase"/>
    <requiredProperty key="modelNameLowerCamel"/>
  </requiredProperties>
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/php</directory>
      <includes>
        <include>**/*.*</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>_post_install_msg.txt</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
